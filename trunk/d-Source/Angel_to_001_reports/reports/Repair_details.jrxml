	<parameter name="start_date" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[null]]></defaultValueExpression>
	</parameter>
	<parameter name="end_date" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[null]]></defaultValueExpression>
